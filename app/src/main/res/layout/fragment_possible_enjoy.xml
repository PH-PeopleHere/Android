<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/tv_possible_enjoy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="@string/block_enjoy"
        android:gravity="center"
        android:textSize="20sp"
        android:fontFamily="@font/pretendard_semibold"
        android:layout_marginTop="36dp"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_possible_enjoy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/block_enjoy_button"
        android:textSize="14sp"
        android:lineHeight="22sp"
        android:textColor="@color/Gray6"
        android:lineSpacingExtra="5sp"
        android:layout_marginTop="27dp"
        android:drawableStart="@drawable/ic_block_enjoy"
        android:drawablePadding="4dp"
        android:fontFamily="@font/pretendard_bold"
        app:layout_constraintTop_toBottomOf="@id/tv_possible_enjoy"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>





</androidx.constraintlayout.widget.ConstraintLayout>